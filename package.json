{
  "name": "@heroku-cli/plugin-endpoints",
  "description": "Heroku Trusted VPC Endpoints CLI",
  "version": "0.0.1",
  "author": "Heroku Data UX",
  "bugs": "https://github.com/heroku/heroku-endpoints-cli/issues",
  "dependencies": {
    "@heroku-cli/color": "1.1.14",
    "@heroku-cli/command": "8.2.2",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "cli-ux": "4.9.3",
    "tslib": "^1"
  },
  "devDependencies": {
    "@fancy-test/nock": "0.1.1",
    "@heroku-cli/schema": "1.0.23",
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "lolex": "3.0.0",
    "mocha": "^5",
    "nock": "10.0.4",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/heroku/heroku-endpoints-cli",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-endpoints-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
