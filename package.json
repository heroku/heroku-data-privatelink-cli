{
  "name": "@heroku-cli/plugin-data-privatelink",
  "description": "Heroku Data via PrivateLink CLI",
  "version": "1.3.2",
  "author": "Heroku",
  "bugs": "https://github.com/heroku/heroku-data-privatelink-cli/issues",
  "dependencies": {
    "@heroku-cli/color": "^2.0.1",
    "@heroku-cli/command": "^11.5.0",
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/core": "^2.16.0",
    "@oclif/plugin-help": "^5",
    "stdout-stderr": "^0.1.13",
    "tslib": "1.14.1"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.15",
    "@types/chai": "^4.3.14",
    "@types/chai-as-promised": "^8.0.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "^25.0.9",
    "@types/supports-color": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "8.53.0",
    "@typescript-eslint/parser": "8.53.0",
    "chai": "^6.2.2",
    "chai-as-promised": "^8.0.2",
    "eslint": "^9.39.2",
    "eslint-config-oclif": "^6.0.132",
    "eslint-config-oclif-typescript": "^3.1.14",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.4.3",
    "eslint-plugin-unicorn": "^62.0",
    "globby": "^16.1.0",
    "lolex": "^6.0.0",
    "mocha": "^11",
    "nock": "^14.0.10",
    "nyc": "^17.1.0",
    "oclif": "^4.14.36",
    "ts-node": "^10.9.2",
    "tsheredoc": "^1.0.1",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/heroku/heroku-data-privatelink-cli",
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "MIT",
  "mocha": {
    "require": [
      "test/helpers/init.js",
      "ts-node/register",
      "source-map-support/register"
    ],
    "watch-extensions": "ts",
    "recursive": true,
    "reporter": "spec",
    "timeout": 30000
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-data-privatelink-cli",
  "scripts": {
    "build": "rm -rf lib && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "rm -rf lib && tsc -b && oclif manifest",
    "pretest": "tsc -p test --noEmit && yarn build",
    "test": "nyc mocha --forbid-only \"test/**/*.unit.test.ts\"",
    "version": "oclif readme && git add README.md"
  }
}
