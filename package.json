{
  "name": "@heroku-cli/plugin-data-privatelink",
  "description": "Heroku Data via PrivateLink CLI",
  "version": "1.3.1",
  "author": "Heroku",
  "bugs": "https://github.com/heroku/heroku-data-privatelink-cli/issues",
  "dependencies": {
    "@heroku-cli/color": "^1.1.14",
    "@heroku-cli/command": "^8.2.6",
    "@oclif/command": "^1.5.8",
    "@oclif/config": "^1.10.4",
    "@oclif/errors": "^1.2.2",
    "@types/supports-color": "^5.3.0",
    "cli-ux": "^5.0.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@heroku-cli/schema": "^1.0.23",
    "@oclif/dev-cli": "^1.21.0",
    "@oclif/plugin-help": "^2.1.4",
    "@oclif/test": "^1.2.2",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "16.11.7",
    "chai": "^4.4.1",
    "globby": "^9.2.0",
    "lolex": "^3.0.0",
    "mocha": "^10.4.0",
    "nock": "^13.5.1",
    "nyc": "^13.1.0",
    "ts-node": "^10.9.2",
    "tslint": "^5.12.0",
    "typescript": "3.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/heroku/heroku-data-privatelink-cli",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "mocha": {
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "watch-extensions": "ts",
    "recursive": true,
    "reporter": "spec",
    "timeout": 360000
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-data-privatelink-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
